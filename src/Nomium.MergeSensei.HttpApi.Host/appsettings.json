{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.Grafana.Loki"
    ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "Enrich": [
      "FromLogContext",
      "WithEnvironmentName",
      "WithThreadId",
      "WithSpan"
    ],
    "WriteTo": [
      {
        "Name": "GrafanaLoki",
        "Args": {
          "uri": "http://localhost:3100",
          "outputTemplate": "{Timestamp:dd-MM-yyyy HH:mm:ss} [{Level}] {Message}{NewLine}{Exception}",
          "labels": [
            {
              "key": "app",
              "value": "Nomium.MergeSensei.HttpApi.Host"
            }
          ],
          "propertiesAsLabels": [
            "app"
          ]
        }
      },
      {
        "Name": "Console"
      }
    ]
  },
  "App": {
    "SelfUrl": "https://localhost:44310",
    "AngularUrl": "http://localhost:4200",
    "CorsOrigins": "https://*.nomium.dev,http://localhost:4200",
    "RedirectAllowedUrls": "http://localhost:4200",
    "DisablePII": false,
    "HealthCheckUrl": "/health-status"
  },
  "ConnectionStrings": {
    "Default": "Host=localhost;Port=5432;Database=sensei;User ID=postgres;Password=password;"
  },
  "AuthServer": {
    "Authority": "https://localhost:44310",
    "RequireHttpsMetadata": true,
    "SwaggerClientId": "MergeSensei_Swagger",
    "CertificatePassPhrase": "b9ead0d7-9076-4f3e-bc8b-def74f58641f"
  },
  "StringEncryption": {
    "DefaultPassPhrase": "8w0aew1P1mgS1IkT"
  }
}